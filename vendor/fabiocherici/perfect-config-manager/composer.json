{
    "name": "fabiocherici/perfect-config-manager",
    "description": "A configuration management library designed for Laravel to streamline handling configuration values with validation support.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fabio\\ConfigManager\\": "src/"
        }
    },
    "authors": [
        {
            "name": "fabio cherici",
            "email": "fabiocherici@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "monolog/monolog": "^3.7",
        "illuminate/support": "^11.29",
        "laravel/framework": "^11.29"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fabio\\PerfectConfigManager\\Providers\\ConfigManagerServiceProvider",
                "Fabio\\PerfectConfigManager\\Providers\\ValidationServiceProvider"
            ]
        }
}
}
